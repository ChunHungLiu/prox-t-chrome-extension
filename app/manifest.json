{
  "name": "Prox-t Beta",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "Enables users to switch to using a different twitch data center.",
  "icons": {
    "16": "images/Logo_16.png",
    "32": "images/Logo_32.png",
    "64": "images/Logo_64.png",
    "128": "images/Logo_128.png",
    "256": "images/Logo_256.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "cookies",
    "*://www.twitch.tv/*",
    "http://*.ttvnw.net/*"
  ],
  "page_action": {
    "default_icon": {
      "19": "images/Logo_19.png",
      "38": "images/Logo_38.png"
    },
    "default_title": "Prox-t Beta",
    "default_popup": "popup.html"
  }
}